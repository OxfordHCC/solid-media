{
  "name": "solid-media",
  "version": "1.0.0",
  "description": "A media profile app build on Solid",
  "main": "index.js",
  "scripts": {
    "start": "snowpack dev --polyfill-node",
    "build": "snowpack build --polyfill-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Luke Miles",
  "snowpack": {
    "mount": {
      "src": "/"
    },
    "routes": [
      {
        "match": "routes",
        "src": ".*",
        "dest": "/index.html"
      }
    ],
    "plugins": [
      "@snowpack/plugin-typescript"
    ],
    "webDependencies": [
      "preact",
      "wouter-preact"
    ]
  },
  "dependencies": {
    "@inrupt/solid-client": "^1.11.0",
    "@inrupt/solid-client-authn-browser": "^1.11.0",
    "@inrupt/vocab-common-rdf": "^1.0.0",
    "preact": "^10.5.14",
    "wouter-preact": "^2.7.4"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "snowpack": "^3.8.6",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 1 Safari version"
  ]
}
